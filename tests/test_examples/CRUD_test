/*select*/
select * from qwe where q = a;
select qe,se from qwe where q = a;
select qe,se from qwe where q = 1;
select qe,se from qwe where q = true;
select qe,se from qwe where q = "qwe";
select qe,se from qwe where q = 1.13;

select * from qwe;

	/*error*/
	select *,id from qwe where q = a;
/*end select*/

/*insert*/
insert into qwe (id,name) values (1,"qqw");
insert into asdtrue (qw,we,er,rt) values (1,1.123,"123",true);
/*end insert*/

/*update*/
update qwe set id = "q" where id = 1;
update qwe set id = 1 where id = 1.5;
update qwe set id = 1.24 where id = "123";
update qwe set id = "q" where id = false;

	/*error*/
	update qwe set id = "q";
/*end update*/

/*delete*/
delete from qwe where id = 1;
delete from qwe where id = 1.2;
delete from qwe where id = "123";
delete from qwe where id = false;
/*end delete*/